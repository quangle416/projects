package com.perscholas.java_basics;

public class MainJavaBasics {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 10;
		int j = 20;
		int x = i+j;
		
		System.out.println(x);
		
		double a = 1.11;
		double b = 2.22;
		double c = a+b;
		
		System.out.println(c);
	}

}
